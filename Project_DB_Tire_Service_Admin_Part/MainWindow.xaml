<Window x:Class="Project_DB_Tire_Service_Admin_Part.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:hamburgerMenu="clr-namespace:HamburgerMenu;assembly=HamburgerMenu"
        xmlns:local="clr-namespace:Project_DB_Tire_Service_Admin_Part"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="1*" />
            <ColumnDefinition Width="1*" />
        </Grid.ColumnDefinitions>
        <hamburgerMenu:HamburgerMenu Grid.Column="0" Background="Blue" MenuIconColor="White" SelectionIndicatorColor="White" MenuItemForeground="White" HorizontalAlignment="Left">
            <hamburgerMenu:HamburgerMenuItem Text="Home" SelectionCommand="{Binding MyCommand}"/>
            <hamburgerMenu:HamburgerMenuItem Text="Search"/>
        </hamburgerMenu:HamburgerMenu>
        <Frame Content="Frame" Grid.Column="0" Margin="55,10,0,0"/>

    </Grid>
</Window>
